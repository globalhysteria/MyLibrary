<label for="title">Title</label>
<input type="text" name="title" id="title" value="<%= book.title %>">
<br>
<label for="description">Description</label>
<input type="text" name="description" id="description" value="<%= book.description %>">
<br>
<label for="publishDate">Publish Date</label>
<input type="date" name="publishDate" id="publishDate" value="<%= book.publishDate %>">
<br>
<label for="pageCount">Page Count</label>
<input type="number" name="pageCount" id="pageCount" value="<%= book.pageCount %>">
<br>
<label for="author">Author</label>
<select name="author" id="author">
    <% authors.forEach(author => { %>
        <option value="<%= author.id %>"><%= author.givenName %> <%= author.familyName %></option>
    <% }) %>
</select>
<br>
<label for="coverImage">Cover Image</label>
<input type="text" name="coverImage" id="coverImage" value="<%= book.coverImage %>">
